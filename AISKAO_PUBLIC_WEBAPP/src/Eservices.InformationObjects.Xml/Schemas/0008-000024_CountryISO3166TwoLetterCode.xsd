<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://ereg.egov.bg/value/0008-000024"
      xmlns="http://ereg.egov.bg/value/0008-000024"
      xmlns:xsd="http://www.w3.org/2001/XMLSchema"
      elementFormDefault="qualified">


	<xsd:simpleType name="CountryISO3166TwoLetterCode">
        <xsd:annotation>
            <xsd:documentation xml:lang="bg">Двубуквен код на държава по ISO 3166</xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
